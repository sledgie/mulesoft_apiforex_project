		<jms:active-mq-connection username="admin" password="admin" >
			<jms:factory-configuration brokerUrl="tcp://192.168.7.242:61616" />
		</jms:active-mq-connection>
	</jms:config>
	<os:config name="ObjectStore_Config" doc:name="ObjectStore Config" doc:id="8283a53d-dd0f-45d0-afc4-7f0477877706" >
		<os:connection />
	</os:config>
	<os:object-store name="Object_store" doc:name="Object store" doc:id="8cb57058-38ef-46f9-a750-1792b558aade" persistent="false" maxEntries="1000" entryTtl="1" entryTtlUnit="DAYS" config-ref="ObjectStore_Config" expirationIntervalUnit="DAYS"/>
	<configuration-properties doc:name="Configuration properties" doc:id="5cacce8f-6588-4ce7-a5df-9bf9c568467e" file="config.yaml" />


